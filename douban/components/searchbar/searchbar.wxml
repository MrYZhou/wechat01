<!--components/serachbar/searchbar.wxml-->
<view class="searchbar" >
  <navigator url='/pages/search/search'
    class='search-navigator' wx:if="{{isNavigator}}"
  ></navigator>
  
  <view wx:else class='search-input-group'>
    <input class="search-input" placeholder="搜索" bindinput="onInputEvent">
    </input>
  </view>
</view>
