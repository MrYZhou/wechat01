/* components/serachbar/searchbar.wxss */

.searchbar{

  background-color: #41be57;
  padding: 20rpx; 
}
.search-navigator{
  width: 100%;
  height:60rpx;
  background-color: #fff;
  border-radius:10rpx;
  background-position:center center;
  background-repeat: no-repeat;
  background-size:5%;
  background-image:url('data:image/png;base64,');

}
.search-input-group{
  width: 100%;
  height:60rpx;
  background-color: #fff;
  border-radius:10rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.search-input{
  /* 这里要设置最小高度，不然height不生效 */
  min-height: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 32rpx;
}